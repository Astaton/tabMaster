{
    "name": "tabMaster",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contents.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "tabMaster"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}